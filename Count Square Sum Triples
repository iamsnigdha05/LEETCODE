class Solution:
    def countTriples(self, n):
        count = 0
        for c in range(1, n+1):
            a = 1
            b = c - 1
            c2 = c * c
            while a < b:
                s = a * a + b * b
                if s == c2:
                    
                    count += 2
                    a += 1
                    b -= 1
                elif s < c2:
                    a += 1
                else:
                    b -= 1
        return count

        
